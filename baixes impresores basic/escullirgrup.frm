End Sub

Private Sub Form_Load()
obrestocks
    comanda = Form1.comanda
    possarnomgrups
End Sub

Sub ensenyargrup(ngrup As Double)
    If ngrup = 0 Then Exit Sub
     Unload escullirgrup
     escriure_ini "baixes", "imprimirpackinglist", "1", "comandes.ini"
     Form1.llistatpackinglist ngrup
     missatgevist.Show 1
     escriure_ini "baixes", "imprimirpackinglist", "0", "comandes.ini"
End Sub
Sub possarnomgrups()
   Dim i As Byte
   Dim rst As Recordset
   Set rst = dbstocks.OpenRecordset("select numerogrup,nomdelgrup from grupsdepalets where seccio='I'")
   For i = 0 To botogrups.Count - 1
     If Not rst.EOF Then
        botogrups(i).caption = atrim(rst!numerogrup) + Chr(10) + UCase(atrim(rst!nomdelgrup))
        botogrups(i).visible = True
        botogrups(i).tag = atrim(rst!numerogrup)
        rst.MoveNext
       Else:
         botogrups(i).caption = ""
         botogrups(i).visible = False
         botogrups(i).tag = ""
     End If
     
   Next i
End Sub
